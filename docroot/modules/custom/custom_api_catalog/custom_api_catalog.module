    switch ($route_name) {
        // Main module help for the custom_api_catalog module.
        case 'help.page.custom_api_catalog':
            $output = '';
            $output .= '<h3>' . t('About') . '</h3>';
            $output .= '<p>' . t('Sync Api Products from Apigee and listing in Developer Portal') . '</p>';
